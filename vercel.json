{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],
  "env": {
    "CONNECTION_STRING": "@connection_string",
    "PORT": "3000",
    "JWT_SECRET": "@jwt_secret"
  }
}
